{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run BlazePose Example (CPU, console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Mediapipe.Net.Examples.BlazePose/bin/Debug/net6.0/Mediapipe.Net.Examples.BlazePose.dll",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Run FaceMesh Example (CPU, console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Mediapipe.Net.Examples.FaceMesh/bin/Debug/net6.0/Mediapipe.Net.Examples.FaceMesh.dll",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Run FaceMesh Example (GPU, console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Mediapipe.Net.Examples.FaceMeshGpu/bin/Debug/net6.0/Mediapipe.Net.Examples.FaceMeshGpu.dll",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Run Visual Tests (CPU, console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Mediapipe.Net.Examples.OsuFrameworkVisualTests/bin/Debug/net6.0/Mediapipe.Net.Examples.OsuFrameworkVisualTests.dll",
            "linux": {
                "env": {
                    "SDL_VIDEO_X11_NET_WM_BYPASS_COMPOSITOR": "0"
                }
            },
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}
